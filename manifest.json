{
  "name": "url-patcher",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Fast switching between similar domains by patching the active tab url using custom replacement rules",
  "homepage_url": "https://github.com/Oppodelldog/chrome-extension-url-patcher",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png",
    "148": "icons/icon-148.png"
  },
  "browser_action": {
    "default_title": "Url Patcher",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "background/storage.js"
    ]
  }
}